{
  "name": "viewport-extra",
  "version": "1.1.1",
  "description": "Enable to set min-width and max-width to viewport",
  "module": "dist/es/index.js",
  "main": "dist/cjs/index.js",
  "browser": "dist/iife/index.min.js",
  "types": "types/index.d.ts",
  "jest": {
    "roots": [
      "dist/__test__/"
    ]
  },
  "scripts": {
    "test": "jest",
    "build": "rollup --config --environment NODE_ENV:production",
    "watch": "rollup --config --environment NODE_ENV:development --watch",
    "dev": "npm run watch"
  },
  "lint-staged": {
    "!(*.{ts,js})": [
      "prettier -w"
    ],
    "*.{ts,js}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsktschy/viewport-extra.git"
  },
  "keywords": [
    "viewport",
    "min",
    "max",
    "width",
    "mobile",
    "device",
    "shrink",
    "expand"
  ],
  "author": "dsktschy (https://github.com/dsktschy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsktschy/viewport-extra/issues"
  },
  "homepage": "https://github.com/dsktschy/viewport-extra#readme",
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "acorn": "^8.0.4",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "globby": "^11.0.1",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "rollup": "^2.32.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=14",
    "npm": "<7"
  }
}
